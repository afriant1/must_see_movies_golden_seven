<h1>Movie #<%= @movie.id %></h1>

<table>
<dt>Title</dt>
<dd><%=@movie.title%></dd>

<dt>Year</dt>
<dd><%= @movie.year %></dd>

<dt>Duration</dt>
<dd><%= @movie.duration %></dd>

<dt>Description</dt>
